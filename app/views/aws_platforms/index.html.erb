<h1>Listing aws_platforms</h1>

<table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>Platform no</th>
      <th>接続先Host</th>
      <th>API待受Port</th>
      <th>HTTPS接続</th>
      <th>Euca互換</th>
      <th>Vpc環境</th>
      <th>Region</th>
      <th>Availability zone</th>
      <th>Vpc id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @aws_platforms.each do |aws_platform| %>
      <tr>
        <td><%= aws_platform.PLATFORM_NO %></td>
        <td><%= aws_platform.HOST %></td>
        <td><%= aws_platform.PORT %></td>
        <td><%= aws_platform.SECURE %></td>
        <td><%= aws_platform.EUCA %></td>
        <td><%= aws_platform.VPC %></td>
        <td><%= aws_platform.REGION %></td>
        <td><%= aws_platform.AVAILABILITY_ZONE %></td>
        <td><%= aws_platform.VPC_ID %></td>
        <td><%= link_to 'Show', aws_platform %></td>
        <td><%= link_to 'Edit', edit_aws_platform_path(aws_platform) %></td>
        <td><%= link_to 'Destroy', aws_platform, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

